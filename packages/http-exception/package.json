{
  "name": "@belgattitude/http-exception",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://github.com/belgattitude/http-exception",
  "repository": {
    "type": "git",
    "url": "https://github.com/belgattitude/http-exception",
    "directory": "packages/http-exception"
  },
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "sideEffects": false,
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.js",
      "module": "./dist/esm/index.js",
      "import": "./dist/esm/index.js",
      "browser": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rimraf --no-glob ./dist && cross-env NODE_ENV=production rollup -c rollup.config.js",
    "clean": "rimraf --no-glob ./dist ./coverage ./tsconfig.tsbuildinfo",
    "build-doc": "rimraf --no-glob ./docs/api && typedoc --plugin typedoc-plugin-markdown --out ./docs/api",
    "prettier-check": "yarn run --top-level prettier --check --config ../../.prettierrc.js --ignore-path ../../.prettierignore \"./**/*.{js,jsx,cjs,mjs,ts,tsx,mts,md,mdx,json,css,scss,less}\"",
    "prettier-fix": "yarn run --top-level prettier --write --config ../../.prettierrc.js --ignore-path ../../.prettierignore \"./**/*.{js,jsx,cjs,mjs,ts,tsx,mts,md,mdx,json,css,scss,less}\"",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../.cache/eslint/errorh.eslintcache",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "check-dist": "npm-run-all -s check-dist-esm check-dist-cjs",
    "check-dist-esm": "es-check --not './dist/esm/*.map.js' -v es2016 --module './dist/esm/**/*'",
    "check-dist-cjs": "es-check --not './dist/cjs/*.map.js' -v es2016 './dist/cjs/**/*'",
    "check-size": "size-limit",
    "test": "vitest run",
    "test-unit": "vitest run",
    "test-unit-watch": "vitest --ui",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --fix"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-classes": "7.19.0",
    "@babel/plugin-transform-spread": "7.19.0",
    "@babel/plugin-transform-typescript": "7.19.1",
    "@babel/preset-env": "7.19.1",
    "@babel/preset-typescript": "7.18.6",
    "@belgattitude/eslint-config-bases": "1.8.0",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "22.0.2",
    "@rollup/plugin-node-resolve": "14.1.0",
    "@size-limit/file": "8.1.0",
    "@size-limit/webpack": "8.1.0",
    "@size-limit/webpack-why": "8.1.0",
    "@types/node": "18.7.18",
    "@types/statuses": "2.0.0",
    "@vitest/coverage-istanbul": "0.23.2",
    "@vitest/ui": "0.23.2",
    "corejs": "1.0.0",
    "cross-env": "7.0.3",
    "es-check": "7.0.1",
    "eslint": "8.23.1",
    "get-tsconfig": "4.2.0",
    "microbundle": "0.15.1",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "rollup": "2.79.0",
    "rollup-plugin-dts": "4.2.2",
    "rollup-plugin-terser": "7.0.2",
    "size-limit": "8.1.0",
    "statuses": "2.0.1",
    "terser": "5.15.0",
    "tslib": "2.4.0",
    "tsup": "6.2.3",
    "typedoc": "0.23.14",
    "typedoc-plugin-markdown": "3.13.6",
    "typescript": "4.8.3",
    "vite": "3.1.1",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.23.2",
    "webpack": "5.74.0"
  },
  "engines": {
    "node": ">=14.16"
  }
}
